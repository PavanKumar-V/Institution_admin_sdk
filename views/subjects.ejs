<%- include('./includes/head.ejs') %>
  <%- include('./includes/nav.ejs') %>
    <div class="wrapper">
      <div class="main-content">
        <%- include('./constants/loader.ejs') %>
          <div class="main-data">
            <h1 class="main-title">
              <%= title %>
            </h1>
            <div class="description">Modify and Add Subjects</div>
            <br />
            <hr />
            <br />
            <div class="display-flex j-start">
              <select name="branch" id="branch" class="m-l">
                <option value="" selected disabled hidden>Choose branch</option>
                <option value="CSE" selected>CSE</option>
                <option value="ECE">ECE</option>
                <option value="ISE">ISE</option>
                <option value="MECH">MECH</option>
                <option value="CIV">CIV</option>
                <option value="ARC">ARC</option>
                <option value="EEE">EEE</option>
              </select>
              <select name="sem" id="sem" class="m-l">
                <option value="" selected disabled hidden>Choose sem</option>
                <option value="1">sem 1</option>
                <option value="2">sem 2</option>
                <option value="3">sem 3</option>
                <option value="4">sem 4</option>
                <option value="5">sem 5</option>
                <option value="6">sem 6</option>
                <option value="7">sem 7</option>
                <option value="8" selected>sem 8</option>
              </select>
            </div>
            <br />

            <div class="row" id="subject-list">

            </div>
          </div>
      </div>
    </div>
    <%- include('./includes/footer.ejs') %>